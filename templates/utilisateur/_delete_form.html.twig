<form method="post" action="{{ path('app_utilisateur_delete', {'id': utilisateur.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cet utilisateur : {{ utilisateur.pseudo }} ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ utilisateur.id) }}">
	<button class="btn">Supprimer</button>
</form>
