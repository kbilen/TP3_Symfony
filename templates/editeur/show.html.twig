{% extends 'base.html.twig' %}

{% block title %}Editeur
{% endblock %}

{% block body %}
	<h1>Editeur</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ editeur.id }}</td>
			</tr>
			<tr>
				<th>Nom</th>
				<td>{{ editeur.nom }}</td>
			</tr>
			<tr>
				<th>Pays</th>
				<td>{{ editeur.pays }}</td>
			</tr>
			<tr>
				<th>SiteWeb</th>
				<td>
					{% if editeur.siteWeb %}
						<a href="{{ editeur.siteWeb }}" target="_blank">{{ editeur.siteWeb }}</a>
					{% endif %}
				</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ editeur.description }}</td>
			</tr>
			<tr>
				<th>Créé le</th>
				<td>{{ editeur.createdAt ? editeur.createdAt|date('d/m/Y H:i') : '' }}</td>
			</tr>
			<tr>
				<th>Mis à jour le</th>
				<td>{{ editeur.updatedAt ? editeur.updatedAt|date('d/m/Y H:i') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_editeur_index') }}">Retour à la liste</a>

	<a href="{{ path('app_editeur_edit', {'id': editeur.id}) }}">Modifier</a>

	{{ include('editeur/_delete_form.html.twig') }}
{% endblock %}
